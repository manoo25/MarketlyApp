{
  "expo": {
    "name": "MarketlyApp",
    "slug": "MarketlyApp",
    "scheme": "marketly",                   
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.marketlyapp"  
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.marketlyapp",         
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "marketly",
            "host": "auth",
            "pathPrefix": "/callback"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-web-browser"]
  }
}
